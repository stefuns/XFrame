<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.stepyen.xframedemo">


    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.stepyen.xfrema.base.BaseApplication"
        >

        <!-- Arms 配置 -->
        <meta-data
            android:name="com.stepyen.xframedemo.app.GlobalConfiguration"
            android:value="ConfigModule"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".SimpleActivity"
            android:label="SimpleActivity"
            ></activity>

        <activity android:name=".mvp.TestMVPActivity"
            android:label="测试MVP"
            ></activity>
    </application>

</manifest>